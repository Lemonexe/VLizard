{
  "name": "vlizard",
  "author": "Jiri Zbytovsky",
  "description": "VLizard, a VLE wizard.",
  "private": true,
  "version": "1.0.0",
  "engines": {
    "npm": ">=10.0.0",
    "node": ">=20.0.0"
  },
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "tsc && vite build && electron-builder",
    "tsc": "tsc",
    "build:vite": "vite build",
    "build:electron": "electron-builder",
    "prettier": "prettier --write \"src/**/*.{js,ts,jsx,tsx}\"",
    "lint": "tsc && eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --fix --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "vitest",
    "preview": "vite preview"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.0.1",
    "@mui/material": "^6.0.1",
    "@tanstack/react-query": "^5.53.1",
    "axios": "^1.7.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.1",
    "react-spreadsheet": "^0.9.5",
    "scheduler": "^0.23.2"
  },
  "devDependencies": {
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "electron": "^32.0.1",
    "electron-builder": "^24.13.3",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.11",
    "prettier": "^3.3.3",
    "tree-kill": "^1.2.2",
    "typescript": "^5.5.4",
    "vite": "^5.4.2",
    "vite-plugin-electron": "^0.28.7",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vitest": "^2.0.5"
  },
  "main": "dist-electron/main.js"
}
