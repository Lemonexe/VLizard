    "@tanstack/react-query": "^5.40.1",
    "axios": "^1.7.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.1",
    "react-spreadsheet": "^0.9.4",
    "scheduler": "^0.23.2"
